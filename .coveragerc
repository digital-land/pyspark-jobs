[run]
source = src/
omit = 
    */tests/*
    */test_*
    */__init__.py
    */setup.py
    */conftest.py
    */Athena-connectivity.py
    */Athena-to-sqlite.py
    */setting_secrets.py
    */sqlite_writer_utils.py
    */postgres_writer_utils_new.py
    */flatten_csv.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if settings.DEBUG
    raise AssertionError
    raise NotImplementedError
    if 0:
    if __name__ == .__main__.:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

show_missing = True
skip_covered = False
precision = 2

[html]
directory = htmlcov